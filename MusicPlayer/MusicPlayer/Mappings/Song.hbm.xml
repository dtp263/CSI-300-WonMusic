<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   auto-import="true"
                   assembly="MusicPlayer"
                   namespace="MusicPlayer">


  <class name="Song" table="Song">
    <id name="ID" column="SongID">
      <generator class="native"/>
    </id>
    <property name="Title"/>
    <property name="LengthSeconds"/>
    <property name="ArtistId"/>
    <property name="AlbumId"/>
    <property name="FormatId"/>
    <property name="PriceUS"/>
    <property name="Rating"/>
    <property name="BPM"/>
    <property name="BitRate"/>
  </class>


</hibernate-mapping>